<% include partial/header %>
<div class="container">
  <div class="jumbotron">
    <h4 style="text-align : center">Add New Image To Campground</h4>
  </div>
    <form action="/campground" method="post">
      <div style="width : 35%; margin : auto">
        <div class="form-group">
          <input type="text" class="form-control mb-2 col" name="name" placeholder="Name">
        </div>
        <div class="form-group">
          <input type="text" class="form-control mb-2 col" name="image" placeholder="Image">
        </div>
        <div class="form-group">
          <input type="text" class="form-control mb-2 col" name="desc" placeholder="Discription">
        </div>
        <div class="form-group">
          <button class="btn-primary rounded col p-1">Submit</button>
        </div>
      </div>
    </form>
    <br>
    <a href="/campground" class="btn-primary rounded p-2">Go Back</a>
</div>
<% include partial/footer %>
